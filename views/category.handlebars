
<div class="container">

  {{#each apetizers as |apetizer|}}
  <div class="col-md-4">
    <p>
      {{apetizer.id}}.
      {{apetizer.name}}
    </p>
  </div>
  
  <div class="col-md-3">
    {{#if apetizer.has_nuts}} Contains nuts {{else}} No nuts {{/if}}
     {{#if apetizer.hasMeat}} Contains Meat {{else}} No Meat {{/if}}
  </div>
  {{/each}}
  </div>